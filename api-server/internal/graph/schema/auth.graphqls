extend type Mutation {
  login(input: LoginInput!): LoginPayload!
  register(input: RegisterInput!): RegisterPayload!
}

input LoginInput {
  username: String!
  password: String!
}
type LoginPayload {
  token: String!
}

input RegisterInput {
  username: String!
  name: String!
  surname: String!
}
type RegisterPayload {
  token: String!
}
